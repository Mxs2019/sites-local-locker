{
  "name": "sites-local-locker",
  "version": "1.0.0",
  "description": "Another deploy",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:webpack watch:sites",
    "watch:webpack": "webpack --mode development --watch",
    "watch:sites": "yext sites develop",
    "build": "export NODE_ENV=production && webpack --mode production",
    "pull-resources": "yext resources pull ./account-config",
    "apply-resources": "yext resources apply ./account-config",
    "download-streams": "yext sites generate-test-data --label=account-3447338/sites && yext sites generate-test-data --label=account-3447338/locations"
  },
