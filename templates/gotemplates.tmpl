{{define "location.url" -}}
{{.externalEntityId}}.html
{{- end}}
{{define "location.source" -}}
<html>
    <body>
        <h1> Welcome to Luc's Electronics - {{.name}}</h1>
        <p>{{.address}}</p>
        <p>Hours: {{.hours}}</p>
    </body>
</html>
{{end}}

{{define "city.url" -}}
{{.externalEntityId}}.html
{{- end}}
{{define "city.source" -}}
<html>
    <body>
        <h1> Welcome to {{.name}}, {{.c_parents.name}}</h1>
        <h2> Parents: {{.c_parents}} </h2>
        <h2> Parents Index name: {{index .c_parents "name"}}</h2>
        <h2> Children: {{.c_children}} </h2>
        <h2> Children Index name: {{index .c_children "name"}}</h2>
        
        {{range .c_parents}}
            <h3>i'm a parent</h3>
        {{end}}

        {{range .c_children}}
            <h3>i'm a child</h3>
        {{end}} 
    </body>
</html>
{{end}}
